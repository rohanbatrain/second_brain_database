# Test configuration for voice integration
# Copy this to .sbd or .env to test LiveKit integration

# LiveKit Configuration (for testing - use your own values)
LIVEKIT_API_KEY=test_api_key
LIVEKIT_API_SECRET=test_api_secret
LIVEKIT_URL=ws://localhost:7880

# Voice Processing Configuration
VOICE_SAMPLE_RATE=16000
VOICE_CHUNK_SIZE=1024
VOICE_MAX_AUDIO_LENGTH=30
VOICE_TTS_VOICE=en

# AI Voice Configuration
AI_VOICE_ENABLED=true
AI_VOICE_STT_ENABLED=true
AI_VOICE_TTS_ENABLED=true
AI_VOICE_REALTIME_ENABLED=true
AI_VOICE_COMMAND_ENABLED=true