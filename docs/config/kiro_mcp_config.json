{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"],
      "env": {},
      "disabled": true,
      "autoApprove": []
    },
    "gofastmcp": {
      "command": "uvx",
      "args": ["gofastmcp"],
      "env": {
        "FASTMCP_LOG_LEVEL": "INFO"
      },
      "disabled": false,
      "autoApprove": []
    },
    "second-brain-database": {
      "transport": "http",
      "url": "http://localhost:3001/mcp",
      "headers": {
        "Authorization": "Bearer dev-token",
        "Content-Type": "application/json"
      },
      "env": {
        "FASTMCP_LOG_LEVEL": "INFO"
      },
      "disabled": false,
      "autoApprove": [
        "get_family_info",
        "list_family_members",
        "get_user_profile",
        "get_system_health"
      ]
    }
  }
}