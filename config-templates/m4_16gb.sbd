{
  "profile": "m4_16gb",
  "description": "High-performance configuration for Apple M4 16GB systems",
  "langgraph": {
    "workers": 8,
    "port": 2024
  },
  "ollama": {
    "num_ctx": 8192,
    "num_predict": 1024,
    "num_gpu": 1,
    "num_thread": 8,
    "repeat_penalty": 1.1,
    "request_timeout": 120.0
  },
  "platform": {
    "batch_size": 16,
    "max_concurrent_requests": 8,
    "enable_neural_engine": true
  },
  "models": {
    "agent": "llama3.2:latest",
    "embeddings": "llama3.2:latest",
    "fallback": "llama3.2:latest"
  },
  "performance": {
    "streaming": true,
    "cache_enabled": true,
    "preload_models": true
  }
}
